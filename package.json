{
  "name": "nodejs-backend-api",
  "version": "1.0.0",
  "description": "Complete Node.js backend with auto-generated Swagger documentation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --delay 2 --ignore config/swagger-output.json --ignore node_modules server.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "swagger": "node scripts/generate-swagger.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "swagger-autogen": "^2.23.7",
    "express-list-endpoints": "^6.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0"
  },
  "nodemonConfig": {
    "ignore": ["config/swagger-output.json", "node_modules/*", "*.log"],
    "delay": 2000,
    "env": {
      "NODE_ENV": "development"
    }
  },
  "keywords": ["nodejs", "express", "mysql", "swagger", "api", "backend"],
  "author": "Your Name",
  "license": "MIT"
}
